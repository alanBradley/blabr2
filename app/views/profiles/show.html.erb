<p id="notice"><%= notice %></p>

<div class="container-fluid">
  <div class="row">
    <div id="content" class="col-md-4 center-block">

      <p>
        <strong>Firstname:</strong>
        <%= @profile.firstname %>
      </p>

      <p>
        <strong>Lastname:</strong>
        <%= @profile.lastname %>
      </p>

      <p>
        <strong>Avatar:</strong>
        <%= image_tag @profile.avatar.url(:thumb) %>
      </p>

      <p>
        <strong>Bio:</strong>
        <%= @profile.bio %>
      </p>

      <p>
        <strong>City:</strong>
        <%= @profile.city %>
      </p>

      <p>
        <strong>Gender:</strong>
        <%= @profile.gender %>
      </p>

      <p>
        <strong>Email:</strong>
        <%= @profile.user.email %>
      </p>

      <%= link_to 'Edit', edit_profile_path(@profile) %> |
      <%= link_to 'Back', profiles_path %>
    </div>
  </div>
</div>


    <div id="content" class="col-md-4 right-block">
